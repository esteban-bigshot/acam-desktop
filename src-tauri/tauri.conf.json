{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "./src",
    "distDir": "./src"
  },
  "package": {
    "productName": "acam",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "scope": [
          "$RESOURCE/*"
        ]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.bigshotapps.acam",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "resources/*"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "visible": false
      }
    ],
    "systemTray": {
      "iconPath": "icons/gfi.png",
      "iconAsTemplate": true
    }
  }
}